    <!-- Donate -->
    <string name="donate_title">Want to buy me a beer?</string>
    <string name="donate_head">Donations are always appreciated and helpful in funding the future development of this and other apps.\n\nHowever, if you find Hangar useful and want to show thanks for the work that has gone into it, visit the Contribute section to see the many ways you can help.\n\nIf you\'re in a generous mood, you can donate using <b>Google Play</b> below.</string>
    <string name="donate_title_google" translatable="false">Google Play</string>
    <string name="donate_google">Donate with Google</string>
    <string name="donate_contribute">Make Hangar better!</string>
    <string name="donate_accept_button">"Keep on keepin\' on"</string>
    <string-array name="entries_google_donate" translatable="false">
        <item>$1 (USD)</item>
        <item>$2 (USD)</item>
        <item>$3 (USD)</item>
        <item>$4 (USD)</item>
        <item>$5 (USD)</item>
    </string-array>
    <string name="donate_sorry">"Hm.. looks like Google Play didn\'t like that.  Sorry :("</string>
    <string name="donate_thanks_title">Thank you!</string>
    <string name="donate_thanks_continue">Back to Hangar</string>
    <string name="donate_thanks">"Your contribution to the development of Hangar is much appreciated! The Android community is made better by people like you who support and motivate developers."</string>

    <!-- Contribute -->
    <string name="contribute_title">Contribute to Hangar</string>

    <string name="contribute_google_title">Rate + Review</string>
